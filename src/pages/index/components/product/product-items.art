{{each items}}
<li class="product-items">
    <a href="./details.html?id={{$value.id}}">
        <p class="product-pic">
            <img src="{{$value.url}}" alt="" class="product-img">
        </p>
        <p class="product-name">{{$value.title}}</p>
        <p class="product-price">{{$value.price}} <span>元起</span></p>
    </a>
</li>
{{/each}}